<h1>All Songs</h1>

<table>

<tr>
  <th>Title</th>
  <th>Release Year</th>
  <th>Released? </th>
  <th>Genre</th>
  <th>Artist Name</th>
</tr>
<tbody>
<% @songs.each do |s| %>

<tr>
 <td><%= s.title %></td>
 <td><%= s.release_year %> </td>
 <td><%= s.released %></td>
 <td><%= s.genre %></td>
 <td><%= s.artist_name %></td>
</tr>

<% end %>
</tbody>
</table>
