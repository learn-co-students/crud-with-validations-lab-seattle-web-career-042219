<%= form_for(@song) do |f| %>
  <p><%=  f.text_field :title, placeholder: "Title" %></p>
  <p>Released?: <%=  f.check_box :released %></p>
  <p><%=  f.text_field :release_year, placeholder: "Release Year" %></p>
  <p><%=  f.text_field :artist_name, placeholder: "Artist" %></p>
  <p><%=  f.text_field :genre, placeholder: "Genre" %></p>

  <%= f.submit %>
<% end %>